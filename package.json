  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "infra"
  ],
  "scripts": {
    "dev": "concurrently -n back,front -c auto \"npm:dev -w backend\" \"npm:dev -w frontend\"",
    "build": "npm run -w backend build && npm run -w frontend build",
